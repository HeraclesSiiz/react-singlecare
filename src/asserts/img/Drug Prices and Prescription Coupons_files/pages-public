if($("#LoginModal").length)var vmAuthentication=new Vue({el:"#LoginModal",mixins:[vmNavigationHelper],data:{login:{data:{Email:"",Password:"",ReturnUrl:""},error:"",isLoading:!1},showForgotPassword:!1,forgot:{data:{Email:""},error:"",success:!1,isLoading:!1}},beforeMount:function(){this.consumeLoginCredentials();this.login.data.ReturnUrl=$("#LoginReturnUrl").val()},mounted:function(){SCSForms.initAll("form")},methods:{submitLogin:function(){return this.login.isLoading=!0,SCSAuth.scService(this.consumeLoginCredentials(),SCSAuth.URLS.LOGIN,this.loginCallback(this))},loginCallback:function(n){return function(t){if(t.Success){var i=t.RedirectUrl||SCSAuth.URLS.DASHBOARD_URL;n.navigateTo(i)}else n.login.error=t.Error,n.login.isLoading=!1}},submitForgotPassword:function(){return this.forgot.isLoading=!0,SCSAuth.scService(this.forgot.data,SCSAuth.URLS.FORGOTPASSWORD,this.forgotPasswordCallback)},forgotPasswordCallback:function(n){n.Success?vmAuthentication.forgot.success=!0:vmAuthentication.forgot.error=n.Error;vmAuthentication.forgot.isLoading=!1},show:function(){$(this.$options.el).modal("show")},getReturnUrl:function(){for(var t=window.location.search.split("&"),n=0;n<t.length;n++)if(t[n].indexOf("ReturnUrl")>-1)return t[n].split("=")[1];return SCSAuth.URLS.DASHBOARD_URL},consumeLoginCredentials:function(){SCSHelper.stringIsNullOrEmpty(this.login.data.ReturnUrl)&&(this.login.data.ReturnUrl=this.getReturnUrl());var n={Email:this.login.data.Email,Password:this.login.data.Password,RedirectUrl:this.login.data.ReturnUrl};return this.login.data.Password=n.Password.split("").map(function(){return"*"}).join(""),n}}});(function(){$(".mobile-nav ul").hide();$(".mobile-nav li ul").prev("a").click(function(){return $(this).toggleClass("open-sub-menu"),$(this).next("ul").slideToggle(100),!1});$(".location-btn").length&&setTimeout(function(){SCSGeolocationHelper.initGeoBtn()},300)})()